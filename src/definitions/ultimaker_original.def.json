    "overrides": {
        "machine_name": { "default_value": "Ultimaker Original" },
        "machine_width": {
            "default_value": 205
        },
        "machine_height": {
            "default_value": 200
        },
        "machine_depth": {
            "default_value": 205
        },
        "machine_center_is_zero": {
            "default_value": false
        },
        "machine_head_with_fans_polygon":
